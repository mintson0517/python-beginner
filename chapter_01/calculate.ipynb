   "source": [
    "#四則演算\n",
    "a = 10\n",
    "b = 3\n",
    "print(\"a =\", a)\n",
    "print(\"b =\", b)\n",
    "print(\"足し算:\", a + b)  # 足し算\n",
    "print(\"引き算:\", a - b)  # 引き算\n",
    "print(\"掛け算:\", a * b)  # 掛け算\n",
    "print(\"割り算:\", a / b)  # 割り算\n",
    "print(\"整数除算:\", a // b) # 整数除算\n",
    "print(\"剰余:\", a % b)  # 剰余\n",
    "print(\"べき乗:\", a ** b) # べき乗"
   ]
  }
